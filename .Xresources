URxvt.scrollBar: false
!URxvt.font: xft:ProggyCleanTT CE:style=Book:antialias=false:size=12
!URxvt.font: xft:DejaVu Sans Mono:size=10
!URxvt.font: xft:Roboto Mono:size=9
URxvt.font: xft:Ubuntu Mono:size=10
#include "/home/maggeych/.terminalcolors/base16-xresources/xresources/base16-atelier-forest-256.Xresources"
URxvt.intensityStyles: false
